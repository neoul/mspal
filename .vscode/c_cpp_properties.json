{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**"
                "${workspaceFolder}/build/_deps/poco-src/Foundation/include",
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++14",
            "intelliSenseMode": "linux-gcc-x64",
            "defines": [
                "BUILDENV_DEBUG=1 ",
                "CORECLR ",
                "DEBUG ",
                "DISABLE_CONTRACTS ",
                "HAVE_CONFIG_H ",
                "HOST_64BIT ",
                "HOST_AMD64 ",
                "HOST_UNIX ",
                "LP64COMPATIBLE ",
                "PIC ",
                "TARGET_64BIT ",
                "TARGET_AMD64 ",
                "TARGET_LINUX ",
                "TARGET_UNIX ",
                "URTBLDENV_FRIENDLY=Debug ",
                "USE_STL ",
                "XSTATE_SUPPORTED ",
                "_DBG ",
                "_DEBUG ",
                "_FILE_OFFSET_BITS=64 ",
                "_GNU_SOURCE ",
                "_REENTRANT ",
                "_THREAD_SAFE"
            ],
            "compilerArgs": [
                "-fno-strict-aliasing",
                "-fwrapv",
                "-Wall",
                "-Wunused",
                "-Wmissing-declarations",
                "-Wpointer-arith",
                "-Wno-cast-qual",
                "-Wwrite-strings",
                "-Wno-switch",
                "-Wno-switch-enum",
                "-Wno-unused-value",
                "-Wno-attributes",
                "-Wno-format-zero-length",
                "-Wno-unused-function",
                "-Werror=return-type",
                "-g",
                "-std=gnu++11",
                "-fPIC",
                "-fshort-wchar",
                "-ffp-contract=off",
                "-O0",
                "-Wall",
                "-Werror=conversion-null",
                "-g",
                "-fno-omit-frame-pointer",
                "-fms-extensions",
                "-fwrapv",
                "-fstack-protector-strong",
                "-Wno-unused-variable",
                "-Wno-unused-value",
                "-Wno-unused-function",
                "-Wno-tautological-compare",
                "-Wno-unknown-pragmas",
                "-Wimplicit-fallthrough",
                "-Wno-invalid-offsetof",
                "-Wno-unused-but-set-variable",
                "-Wno-uninitialized",
                "-Wno-strict-aliasing",
                "-Wno-array-bounds",
                "-Wno-misleading-indentation",
                "-Wno-stringop-overflow",
                "-Wno-restrict",
                "-Wno-stringop-truncation",
                "-Wno-class-memaccess",
                "-Wno-placement-new",
                "-faligned-new",
                "-fsigned-char",
                "-fvisibility=hidden",
                "-ffunction-sections",
                "-fexceptions",
                "-Wa,--divide",
                "-MD",
                "-MT"
            ]
        }
    ],
    "version": 4
}